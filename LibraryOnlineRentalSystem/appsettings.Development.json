{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "LibraryDatabase": "Server=libraryorsdb-desofs.mysql.database.azure.com;;Port=3306;Database=${MYSQL_DATABASE};User=${DB_USER};Password=${DB_PASSWORD};"
  },
  "Keycloak": {
    "Authority": "http://keycloak:8080/realms/library",
    "Audience": "library-client",
    "ClientId": "library-client",
    "ClientSecret": "${KEYCLOAK_CLIENT_SECRET}"
  }
}
